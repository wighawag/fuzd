{
  "name": "fuzd-tests",
  "version": "0.2.1",
  "private": true,
  "description": "test client for fuzd",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "keywords": [],
  "author": "",
  "license": "MIT",
  "volta": {
    "node": "18.7.0"
  },
  "dependencies": {
    "@nomicfoundation/ethereumjs-rlp": "^5.0.4",
    "@scure/bip32": "^1.4.0",
    "@scure/bip39": "^1.3.0",
    "atomikv": "workspace:*",
    "fuzd-executor": "workspace:*",
    "fuzd-scheduler": "workspace:*",
    "fuzd-server": "workspace:*",
    "libsql": "^0.3.18",
    "named-logs": "^0.2.4",
    "remote-sql-libsql": "^0.0.3",
    "viem": "^2.9.31"
  },
  "devDependencies": {
    "@noble/hashes": "^1.4.0",
    "@nomicfoundation/hardhat-network-helpers": "^1.0.10",
    "@rocketh/deploy-proxy": "^0.10.6",
    "@rocketh/export": "^0.10.7",
    "@rocketh/signer": "^0.10.5",
    "@rocketh/verifier": "^0.10.7",
    "@types/chai": "^4.3.15",
    "@types/chai-as-promised": "^7.1.8",
    "@types/mocha": "^10.0.6",
    "@types/node": "^20.12.8",
    "abitype": "^1.0.2",
    "as-soon": "^0.0.9",
    "chai": "^5.1.0",
    "chai-as-promised": "^7.1.1",
    "eip-1193": "^0.5.0",
    "eip-1193-jsonrpc-provider": "^0.3.0",
    "eip-1193-signer": "^0.0.2",
    "fuzd-default-client": "workspace:*",
    "hardhat": "^2.22.3",
    "hardhat-rocketh": "^4.0.17",
    "ldenv": "^0.3.10",
    "named-logs-console": "^0.3.1",
    "prettier": "^3.2.5",
    "remote-account": "workspace:*",
    "rocketh": "^0.10.13",
    "set-defaults": "^0.0.2",
    "solidity-coverage": "^0.8.12",
    "solidity-proxy": "^0.4.0",
    "tlock-js": "^0.9.0",
    "ts-node": "^10.9.2",
    "tsup": "^8.0.2",
    "tsx": "^4.9.0",
    "typescript": "^5.4.5",
    "viem": "^1.0.2",
    "viem-chai-matchers": "^0.0.4"
  },
  "scripts": {
    "prepare": "pnpm compile",
    "build": "tsup src/index.ts --dts --format esm,cjs",
    "dev": "tsup src/index.ts --dts --format esm,cjs --watch",
    "format:check": "prettier --check .",
    "format": "prettier --write .",
    "execute": "NAMED_LOGS_LEVEL=6 NAMED_LOGS=fuzd* tsx src/index.ts",
    "local_node": "rm -Rf deployments/localhost && hardhat node",
    "compile": "hardhat compile",
    "watch_compile": "as-soon -w src pnpm compile",
    "deploy": "ldenv hardhat --network @@MODE deploy @@",
    "watch_deploy": "as-soon -w generated -w deploy pnpm run deploy",
    "test": "hardhat test",
    "export": "rocketh-export",
    "start": "zellij --layout zellij.kdl a $npm_package_name || zellij --layout zellij.kdl -s $npm_package_name",
    "stop": "zellij kill-session $npm_package_name"
  }
}
