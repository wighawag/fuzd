{
  "name": "fuzd-tests",
  "version": "0.1.6",
  "private": true,
  "description": "test client for fuzd",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "keywords": [],
  "author": "",
  "license": "MIT",
  "volta": {
    "node": "18.7.0"
  },
  "dependencies": {
    "@nomicfoundation/ethereumjs-rlp": "^5.0.2",
    "@scure/bip32": "^1.3.2",
    "@scure/bip39": "^1.2.1",
    "atomikv": "workspace:*",
    "fuzd-executor": "workspace:*",
    "fuzd-gateways": "workspace:*",
    "fuzd-scheduler": "workspace:*",
    "named-logs": "^0.2.2",
    "tlock-js": "^0.6.1",
    "viem": "^1.10.4"
  },
  "devDependencies": {
    "@noble/hashes": "^1.3.2",
    "@nomicfoundation/hardhat-network-helpers": "^1.0.9",
    "@types/chai": "^4.3.6",
    "@types/chai-as-promised": "^7.1.6",
    "@types/mocha": "^10.0.1",
    "@types/node": "^20.5.9",
    "abitype": "^0.9.8",
    "as-soon": "^0.0.4",
    "chai": "^4.3.8",
    "chai-as-promised": "^7.1.1",
    "eip-1193": "^0.4.7",
    "eip-1193-json-provider": "^0.1.8",
    "eip-1193-signer": "^0.0.1",
    "fuzd-default-client": "workspace:*",
    "hardhat": "^2.17.2",
    "hardhat-rocketh": "^0.6.18",
    "ldenv": "^0.3.7",
    "named-logs-console": "^0.3.0",
    "prettier": "^3.0.3",
    "remote-account": "workspace:*",
    "rocketh": "^0.6.10",
    "rocketh-deploy-proxy": "^0.6.15",
    "rocketh-export": "^0.6.10",
    "rocketh-signer": "^0.6.10",
    "rocketh-verifier": "^0.6.10",
    "set-defaults": "^0.0.1",
    "solidity-coverage": "^0.8.4",
    "solidity-proxy": "^0.2.3",
    "ts-node": "^10.9.1",
    "tsup": "^7.2.0",
    "tsx": "^3.12.8",
    "typescript": "^5.2.2",
    "viem": "^1.0.2",
    "viem-chai-matchers": "^0.0.3"
  },
  "scripts": {
    "prepare": "pnpm compile",
    "build": "tsup src/index.ts --dts --format esm,cjs",
    "dev": "tsup src/index.ts --dts --format esm,cjs --watch",
    "format:check": "prettier --check .",
    "format": "prettier --write .",
    "execute": "NAMED_LOGS_LEVEL=6 NAMED_LOGS=fuzd* tsx src/index.ts",
    "local_node": "rm -Rf deployments/localhost && hardhat node",
    "compile": "hardhat compile",
    "watch_compile": "as-soon -w src pnpm compile",
    "deploy": "ldenv hardhat --network @@MODE deploy @@",
    "watch_deploy": "as-soon -w generated -w deploy pnpm run deploy",
    "test": "hardhat test",
    "export": "rocketh-export",
    "start": "zellij --layout zellij.kdl a $npm_package_name || zellij --layout zellij.kdl -s $npm_package_name",
    "stop": "zellij kill-session $npm_package_name"
  }
}
