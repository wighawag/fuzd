{
  "name": "fuzd-tlock-decrypter",
  "version": "0.2.4",
  "description": "decrypt scheduled execution using drand's tlock-js",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.cjs"
      },
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.mjs"
      }
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "dependencies": {
    "fuzd-scheduler": "workspace:*",
    "named-logs": "^0.2.2",
    "tlock-js": "^0.7.0"
  },
  "devDependencies": {
    "@types/node": "^20.11.17",
    "ldenv": "^0.3.9",
    "pkgroll": "^2.0.2",
    "prettier": "^3.2.5",
    "typescript": "^5.3.3"
  },
  "scripts": {
    "build": "pkgroll",
    "dev": "pkgroll --watch",
    "format:check": "prettier --check .",
    "format": "prettier --write ."
  }
}
