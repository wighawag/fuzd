{
  "name": "fuzd-tlock-decrypter",
  "version": "0.6.0",
  "description": "decrypt scheduled execution using drand's tlock-js",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.cjs"
      },
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.mjs"
      }
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "dependencies": {
    "fuzd-scheduler": "workspace:*",
    "named-logs": "^0.3.0",
    "tlock-js": "npm:@bug-fix/tlock-js@0.9.0-fix.3"
  },
  "devDependencies": {
    "@types/node": "^22.7.9",
    "ldenv": "^0.3.10",
    "pkgroll": "^2.5.0",
    "prettier": "^3.3.3",
    "typescript": "^5.6.3"
  },
  "scripts": {
    "build": "pkgroll",
    "dev": "pkgroll --watch",
    "format:check": "prettier --check .",
    "format": "prettier --write ."
  }
}
