{
  "name": "fuzd-gateways",
  "version": "0.2.1",
  "description": "verify submission and execute them",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.cjs"
      },
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.mjs"
      }
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "dependencies": {
    "atomikv": "workspace:^",
    "fuzd-executor": "workspace:*",
    "fuzd-scheduler": "workspace:*",
    "named-logs": "^0.2.4",
    "viem": "^2.9.31",
    "wighawag-ono": "^7.1.3"
  },
  "devDependencies": {
    "ldenv": "^0.3.10",
    "pkgroll": "^2.0.2",
    "prettier": "^3.2.5",
    "typescript": "^5.4.5"
  },
  "scripts": {
    "build": "pkgroll",
    "dev": "pkgroll --watch",
    "format:check": "prettier --check .",
    "format": "prettier --write ."
  }
}
