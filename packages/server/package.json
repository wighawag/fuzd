{
  "name": "fuzd-server",
  "version": "0.2.4",
  "description": "fuzd server app",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.cjs"
      },
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.mjs"
      }
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "dependencies": {
    "@hono/zod-validator": "^0.2.1",
    "@scure/bip32": "^1.3.3",
    "@scure/bip39": "^1.2.2",
    "eip-1193": "^0.5.0",
    "eip-1193-jsonrpc-provider": "^0.3.0",
    "eip-1193-signer": "^0.0.1",
    "fuzd-common": "workspace:*",
    "fuzd-executor": "workspace:*",
    "fuzd-gateways": "workspace:*",
    "fuzd-scheduler": "workspace:*",
    "fuzd-tlock-decrypter": "workspace:*",
    "hono": "^4.1.5",
    "named-logs": "0.2.4",
    "remote-account": "workspace:*",
    "remote-sql": "^0.0.3",
    "tlock-js": "^0.7.0",
    "zod": "^3.23.4"
  },
  "devDependencies": {
    "ldenv": "^0.3.9",
    "pkgroll": "^2.0.2",
    "prettier": "^3.2.5",
    "typescript": "^5.3.3"
  },
  "scripts": {
    "build": "pkgroll",
    "dev": "pkgroll --watch",
    "format:check": "prettier --check .",
    "format": "prettier --write ."
  }
}
