{
  "name": "fuzd-client",
  "version": "0.7.0",
  "description": "default client for fuzd using tlock-js",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/esm/index.d.ts",
      "default": "./dist/esm/index.js"
    }
  },
  "bin": {
    "fuzd": "dist/esm/cli.js"
  },
  "files": [
    "dist",
    "src"
  ],
  "dependencies": {
    "commander": "^12.1.0",
    "fuzd-chain-protocol": "workspace:*",
    "fuzd-common": "workspace:*",
    "fuzd-executor": "workspace:*",
    "fuzd-scheduler": "workspace:*",
    "named-logs": "^0.3.0",
    "remote-account": "^0.2.1",
    "tlock-js": "npm:@bug-fix/tlock-js@0.9.0-fix.9",
    "viem": "^2.21.37"
  },
  "devDependencies": {
    "@types/node": "^22.8.4",
    "as-soon": "^0.0.9",
    "ldenv": "^0.3.10",
    "prettier": "^3.3.3",
    "tsx": "^4.19.2",
    "typescript": "^5.6.3"
  },
  "scripts": {
    "build": "tsc --project tsconfig.json",
    "dev": "as-soon -w src pnpm build",
    "format:check": "prettier --check .",
    "format:write": "prettier --write .",
    "tsx": "tsx"
  }
}
