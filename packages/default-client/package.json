{
  "name": "fuzd-default-client",
  "version": "0.1.15",
  "description": "default client for fuzd using tlock-js",
  "publishConfig": {
    "access": "public"
  },
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "volta": {
    "node": "18.7.0"
  },
  "dependencies": {
    "fuzd-executor": "workspace:*",
    "fuzd-scheduler": "workspace:*",
    "named-logs": "^0.2.2",
    "remote-account": "workspace:*",
    "tlock-js": "^0.7.0",
    "viem": "^2.7.9"
  },
  "devDependencies": {
    "ldenv": "^0.3.9",
    "prettier": "^3.2.5",
    "tsup": "^8.0.2",
    "tsx": "^4.7.1",
    "typescript": "^5.3.3"
  },
  "scripts": {
    "build": "tsup src/index.ts --dts --sourcemap --format esm,cjs",
    "dev": "tsup src/index.ts --dts --sourcemap --format esm,cjs --watch",
    "format:check": "prettier --check .",
    "format": "prettier --write .",
    "tsx": "tsx"
  }
}
