{
  "name": "fuzd-chain-protocol",
  "version": "0.6.0",
  "description": "protocol used by fuzd to interact with a chain",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "type": "module",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts"
    },
    "./ethereum": {
      "require": {
        "types": "./dist/ethereum/index.d.ts",
        "default": "./dist/ethereum/index.cjs"
      },
      "import": {
        "types": "./dist/ethereum/index.d.ts",
        "default": "./dist/ethereum/index.mjs"
      }
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "dependencies": {
    "@noble/hashes": "^1.4.0",
    "eip-1193-signer": "^0.0.2",
    "fuzd-common": "workspace:*",
    "remote-account": "^0.1.0",
    "remote-procedure-call": "^0.0.12",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "eip-1193": "^0.6.1",
    "ldenv": "^0.3.10",
    "pkgroll": "^2.0.2",
    "prettier": "^3.2.5",
    "typescript": "^5.4.5"
  },
  "scripts": {
    "build": "pkgroll",
    "dev": "pkgroll --watch",
    "format:check": "prettier --check .",
    "format": "prettier --write ."
  }
}
