{
  "name": "fuzd-chain-protocol",
  "version": "0.6.0",
  "description": "protocol used by fuzd to interact with a chain",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "type": "module",
  "types": "./dist/esm/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/esm/index.d.ts"
    },
    "./ethereum": {
      "require": {
        "types": "./dist/cjs/ethereum/index.d.ts",
        "default": "./dist/cjs/ethereum/index.js"
      },
      "import": {
        "types": "./dist/esm/ethereum/index.d.ts",
        "default": "./dist/esm/ethereum/index.js"
      }
    },
    "./starknet": {
      "require": {
        "types": "./dist/cjs/starknet/index.d.ts",
        "default": "./dist/cjs/starknet/index.cs"
      },
      "import": {
        "types": "./dist/esm/starknet/index.d.ts",
        "default": "./dist/esm/starknet/index.js"
      }
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "dependencies": {
    "@noble/hashes": "^1.5.0",
    "@scure/bip32": "^1.5.0",
    "@scure/starknet": "^1.0.0",
    "eip-1193-signer": "^0.0.5",
    "fuzd-common": "workspace:*",
    "remote-account": "^0.2.1",
    "remote-procedure-call": "^0.0.13",
    "starknet-core": "^0.0.3",
    "strk": "^0.0.15",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@starknet-io/types-js": "^0.7.7",
    "eip-1193": "^0.6.1",
    "ldenv": "^0.3.10",
    "as-soon": "^0.0.9",
    "prettier": "^3.3.3",
    "typescript": "^5.6.3"
  },
  "scripts": {
    "build": "pnpm build:esm && pnpm build:cjs",
    "build:esm": "tsc --project tsconfig.json",
    "build:cjs": "tsc --project tsconfig-cjs.json",
    "build:watch": "as-soon -w src pnpm build",
    "dev": "as-soon -w src pnpm build:esm",
    "format:check": "prettier --check .",
    "format": "prettier --write ."
  }
}
