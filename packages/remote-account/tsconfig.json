{
	"compilerOptions": {
		"moduleResolution": "node16",
		"lib": ["ES2020", "dom"],
		"target": "ES2020",
		"declaration": true,
		"declarationMap": true,
		"sourceMap": true,
		"strict": true,
		"esModuleInterop": true,
		"skipLibCheck": true,
		"resolveJsonModule": true,
		"module": "Node16"
	}
}
