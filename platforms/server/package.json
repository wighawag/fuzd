    "named-logs-console": "^0.3.0",
    "tsup": "^8.0.2",
    "typescript": "^5.3.3"
  },
  "scripts": {
    "build": "tsup --entry src/index.ts --entry src/cli.ts --dts --format esm,cjs",
    "dev": "tsup --entry src/index.ts --entry src/cli.ts --dts --watch --format esm,cjs",
