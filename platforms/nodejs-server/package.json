{
  "name": "dreveal-server",
  "version": "0.0.0",
  "publishConfig": {
    "access": "public"
  },
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "dreveal": "dist/cli.cjs"
  },
  "dependencies": {
    "commander": "^10.0.0",
    "ldenv": "^0.3.0",
    "eip-1193-json-provider": "^0.1.0",
    "named-logs": "^0.2.2"
  },
  "devDependencies": {
    "named-logs-console": "^0.2.3",
    "@types/fs-extra": "^11.0.1",
    "@types/node": "^18.15.5",
    "eip-1193": "^0.4.7",
    "fs-extra": "^11.1.1",
    "tsup": "^6.7.0",
    "typescript": "^4.9.5"
  },
  "scripts": {
    "build": "tsup --entry src/index.ts --entry src/cli.ts --dts --format esm,cjs",
    "dev": "tsup --entry src/index.ts --entry src/cli.ts --dts --watch --format esm,cjs",
    "test": "ldenv -m localhost node ./dist/cli.cjs"
  }
}
